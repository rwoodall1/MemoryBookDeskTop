<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- The identifier that must be unique within the hosting gallery -->
        <id>$id$</id>
        <title>ShippingAPI Version 2</title>

        <!-- The package version number that is used when resolving dependencies -->
        <version>$version$</version>

        <!-- Authors contain text that appears directly on the gallery -->
        <authors>$author$</authors>

        <!-- Owners are typically nuget.org identities that allow gallery
             users to earily find other packages by the same owners.  -->
        <owners>$author$</owners>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
       

        <!-- The description can be used in package manager UI. Note that the
             nuget.org gallery uses information you add in the portal. -->
         <description>Package that provides a local interface to the Pitney Bowes Shipping API. Features and fluent API, strong type support, mocking, linq interface to transaction reports.</description>
        <copyright>Copyright 2019 Pitney Bowes</copyright>
    <tags>Pitney Bowes shipping</tags>
    <repository type="Github" url="https://github.com/PitneyBowes/pitneybowes-shipping-api-csharp-openapi" />
        <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/PitneyBowes/pitneybowes-shipping-api-csharp-openapi</projectUrl>

        <!-- Dependencies are automatically installed when the package is installed -->
        <dependencies>

            <dependency id="NewtonSoft.Json" version="12.0.3" />
            <dependency id="JsonSubTypes" version="1.6.0" />
            <dependency id="RestSharp" version="105.1.0" />

        </dependencies>
    </metadata>
    <files>

        <!-- A readme.txt will be displayed when the package is installed -->
        <file src="..\..\README.md" target="" />
        <file src="..\..\docs\**\*.*" target="docs" />

    </files>
</package>
